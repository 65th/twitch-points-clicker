{
  "name": "Twitch Points Clicker",
  "version": "1.0",
  "description": "Clicks that annoying points button.",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.twitch.tv/*"
      ],
      "js": [
        "farm-points.js"
      ],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "manifest_version": 2
}